{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"subcommand": "run",
			"problemMatcher": [
				"$rustc"
			],
			"label": "Rust: cargo run - ClipboardManager",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"dependsOn":["inspector"]
			
		},
		{
			"label": "inspector",
			"type": "shell",
			"command": "startInspector.bat",
			"isBackground": true,
		}
	]
}